         <tr>
            <td>{{ inventory_hostname }}</td>
            <td>
               <ul>
{% for win_update in list_of_updates.updates | dict2items %}
                  {% if win_update|length >=1 %}
                  <li>{{ win_update.value.title }}</li>
		  {% else %}
		  {{ "No updates available" }}
		  {% endif %}
{% endfor %}
               </ul>
            </td>
	    <td>
	    <ul>
	    {{ var_diskspace.size_remaining/1024|pow(3)|float }}
	    </ul>
	    </td>
	    <td>
               <ul>
                  {{ pending_reboot.stdout_lines[0] }}
               </ul>
            </td>	
         </tr>
